{
  "name": "Homegear",
  "version": "0.0.1",
  "slug": "ha-homegear",
  "description": "Homegear Home Assistant Addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "before",
  "boot": "auto",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": ["config:rw", "share:rw"],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
    "/dev/ttyACM0:/dev/ttyACM0:rwm"
  ]
}
